{
    "name": "charlesportwoodii/yii2-psr7-bridge",
    "description": "A PSR7 Bridge for Yii2",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Charles R. Portwood II",
            "email": "charlesportwoodii@erianna.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "^2.0.15",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "laminas/laminas-diactoros": "^2.3",
        "yidas/yii2-bower-asset": "2.0.13.1"
    },
    "require-dev": {
        "samdark/yii2-psr-log-target": "^1.0",
        "monolog/monolog": "~2.1.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpunit/phpunit": "^9.0",
        "spiral/roadrunner": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\Psr7\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii\\Psr7\\tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-compose": true,
            "yiisoft/yii2-composer": true
        }
    }
}
